doctype html
html
  head
    title Soundgates
    link(rel='stylesheet', href='http://fonts.googleapis.com/css?family=Roboto:400,300,700')
    link(rel='stylesheet', href='/stylesheets/bootplus.min.css')
    link(rel='stylesheet', href='/stylesheets/bootplus-responsive.min.css')
    link(rel='stylesheet', href='/stylesheets/nv.d3.css')
    //- link(rel='stylesheet', href='/stylesheets/c3.css')
    link(rel='stylesheet', href='/stylesheets/style.css')
    link(rel='shortcut icon', href='../../assets/ico/favicon.ico')
    body
    .navbar.navbar-inverse.navbar-fixed-top
      .navbar-inner
        .container-fluid
          button.btn.btn-navbar(type='button', data-toggle='collapse', data-target='.nav-collapse')
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.brand(href='#') Soundgates Debugger
          .nav-collapse.collapse
            ul.nav
              li.active
                a(id='home', href='#') Home
              li
                a(href='#about') About
              li
                a(href='#contact') Contact
          // /.nav-collapse
    .container-fluid
      .row-fluid
        .span2
          .sidebar_parent
            #sidebar.nav.nav-pills.nav-stacked.affix(data-clampedwidth=".sidebar_parent")
              ul.nav.nav-list(id='nav_executions')
                li.nav-header Executions
                li.active
                  a(href='#') Link
              ul.nav.nav-list(id='nav_components')
                li.nav-header Components
                li
                  a(href='#') Link
              ul.nav.nav-list
                li.nav-header Debugging
                li
                  a(href='') Refresh
                li
                  a(href='#') Generate
                li
                  a(href='removeEveryExecution()') Remove
            // /.well
          // /span
        .span10
          .hero-unit.shadow
            h1 Soundgates Debugger
          .row-fluid
            .span4
              .card
                h8.card-heading.simple Execution Date
                .card-body(id="execution_date")
                  h1.text-right
                    <b>12:35</b>
                  p.text-right
                    | 2 / March / 2014
            .span4
              .card
                h8.card-heading.simple Components
                .card-body(id="component_count")
            .span4
              .card
                h8.card-heading.simple Implementations
                .card-body(id="component_implementations")
            .span12
              .card
                h8.card-heading.simple Average Execution Time
                .card-body(id="component_average_execution")
            #execution
      hr
  footer
    p Â© Soundgates 2014
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js')
    script(src='/javascripts/d3.js')
    //- script(src='/javascripts/c3.min.js')      
    script(src='/javascripts/nv.d3.js')
    script(src='/javascripts/bootstrap.min.js')
    script(src='/javascripts/offcanvas.js')