<?xml version="1.0" encoding="UTF-8"?>
<gmfmap:Mapping
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    xmlns:gmfmap="http://www.eclipse.org/gmf/2008/mappings"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <nodes>
    <containmentFeature
        href="soundgates.ecore#//Patch/elements"/>
    <ownedChild>
      <domainMetaElement
          href="soundgates.ecore#//AtomicSoundComponent"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="soundgates.gmfgraph#AtomicSoundComponentName"/>
        <features
            href="soundgates.ecore#//NamedElement/name"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="soundgates.gmftool#//@palette/@tools.0/@tools.0"/>
      <diagramNode
          href="soundgates.gmfgraph#AtomicSoundComponent"/>
      <children
          referencedChild="//@nodes.0/@ownedChild/@children.0/@ownedChild">
        <containmentFeature
            href="soundgates.ecore#//SoundComponent/ports"/>
        <ownedChild>
          <domainMetaElement
              href="soundgates.ecore#//Port"/>
          <labelMappings
              xsi:type="gmfmap:FeatureLabelMapping">
            <diagramLabel
                href="soundgates.gmfgraph#AtomicSoundComponentName"/>
            <features
                href="soundgates.ecore#//NamedElement/name"/>
          </labelMappings>
          <tool
              xsi:type="gmftool:CreationTool"
              href="soundgates.gmftool#//@palette/@tools.1/@tools.0"/>
          <diagramNode
              href="soundgates.gmfgraph#Port"/>
        </ownedChild>
      </children>
    </ownedChild>
  </nodes>
  <nodes>
    <containmentFeature
        href="soundgates.ecore#//Patch/elements"/>
    <ownedChild>
      <domainMetaElement
          href="soundgates.ecore#//CompositeSoundComponent"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="soundgates.gmfgraph#CompositeSoundComponentName"/>
        <features
            href="soundgates.ecore#//NamedElement/name"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="soundgates.gmftool#//@palette/@tools.0/@tools.1"/>
      <diagramNode
          href="soundgates.gmfgraph#CompositeSoundComponent"/>
      <children
          referencedChild="//@nodes.0/@ownedChild/@children.0/@ownedChild">
        <containmentFeature
            href="soundgates.ecore#//SoundComponent/ports"/>
      </children>
    </ownedChild>
  </nodes>
  <links>
    <domainMetaElement
        href="soundgates.ecore#//Link"/>
    <containmentFeature
        href="soundgates.ecore#//Patch/elements"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="soundgates.gmftool#//@palette/@tools.2/@tools.0"/>
    <diagramLink
        href="soundgates.gmfgraph#Link"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="soundgates.ecore#//Connection/source"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="soundgates.ecore#//Connection/target"/>
  </links>
  <diagram>
    <diagramCanvas
        href="soundgates.gmfgraph#soundgates"/>
    <domainModel
        href="soundgates.ecore#/"/>
    <domainMetaElement
        href="soundgates.ecore#//Patch"/>
    <palette
        href="soundgates.gmftool#//@palette"/>
  </diagram>
</gmfmap:Mapping>
